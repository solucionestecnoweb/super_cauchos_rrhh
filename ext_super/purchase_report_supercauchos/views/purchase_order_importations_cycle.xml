<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">
        <!-- Add you code here -->

        <!-- purchase.order.importations.cycle tree view -->
        <record id="purchase_order_importations_cycle_view_tree" model="ir.ui.view">
            <field name="name">purchase.order.importations.cycle.view.tree</field>
            <field name="model">purchase.order.importations.cycle</field>
            <field name="arch" type="xml">
                <tree>
                    <!-- Add your fields here -->
                    <field name="partner_id"/>
                    <field name="cycle_brand"/>
                    <field name="invoice_number"/>
                    <field name="cycle_type"/>
                    <field name="prof_date"/>
                    <field name="invoice_id"/>
                    <field name="cont"/>
                    <field name="bl_date"/>
                    <field name="transc"/>
                    <field name="acumulated"/>
                    <field name="eta"/>
                    <field name="traffic"/>
                    <field name="nac"/>
                    <field name="total"/>
                </tree>
            </field>
        </record>

        <!-- purchase.order.importations.cycle form view -->
        <record id="purchase_order_importations_cycle_view_form" model="ir.ui.view">
            <field name="name">purchase.order.importations.cycle.view.form</field>
            <field name="model">purchase.order.importations.cycle</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group string="Cycle Data">
                            <group>
                                <field name="partner_id"/>
                                <field name="cycle_brand"/>
                                <field name="invoice_number"/>
                                <field name="cycle_type"/>
                                <field name="prof_date"/>
                                <field name="invoice_id"/>
                                <field name="cont"/>
                            </group>
                            <group>
                                <field name="bl_date"/>
                                <field name="transc"/>
                                <field name="acumulated"/>
                                <field name="eta"/>
                                <field name="traffic"/>
                                <field name="nac"/>
                                <field name="total"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- purchase.order.importations.cycle search view -->
        <record id="purchase_order_importations_cycle_view_search" model="ir.ui.view">
            <field name="name">purchase.order.importations.cycle.view.search</field>
            <field name="model">purchase.order.importations.cycle</field>
            <field name="arch" type="xml">
                <search string="Import Cycle Search">
                    <!-- Add your fields here -->
                    <field name="partner_id"/>
                    <field name="invoice_number"/>
                    <field name="prof_date"/>
                    <field name="invoice_id"/>
                    <field name="bl_date"/>
                </search>
            </field>
        </record>

    </data>
</odoo>