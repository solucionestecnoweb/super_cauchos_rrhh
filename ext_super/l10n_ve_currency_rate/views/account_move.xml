<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="view_move_form" model="ir.ui.view">
            <field name="name">account.move.form.inherit</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_id']" position="after"> 
                    <field name="custom_rate"/>
               		<field name="os_currency_rate" attrs="{'readonly': [('custom_rate', '=', False)]}"/>
                    <field name="move_aux_id" invisible="1"/>
               </xpath>
               <!--xpath expr="//button[@id='account_invoice_payment_btn']" position="after">
                         <button name="corrige_tasa" string="Recalcular" type="object" />
            </xpath-->
            </field>
        </record>
    </data>
</odoo>