                                    <t t-else="">
                                        <t t-foreach="o.get_imports()" t-as="item">
                                            <tr>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.name.name"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.importation_brand.name"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.prof"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.importation_type"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;" t-if="item.f_prof"><t t-esc="item.f_prof.strftime('%d/%m/%y')"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.invoice_id.name"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.container" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;" t-if="item.bl_date"><t t-esc="item.bl_date.strftime('%d/%m/%y')"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.transc" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.accumulated" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;" t-if="item.eta"><t t-esc="item.eta.strftime('%d/%m/%y')"/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.transit" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.nac" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span>
                                                </td>
                                                <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-center">
                                                    <span style="font-size:12px;"><t t-esc="item.total" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span>
                                                </td>
                                            </tr>
                                            <t t-set="t_cont" t-value="t_cont + item.container"/>
                                            <t t-if="item.nac">
                                                <t t-set="t_accumulated" t-value="item.accumulated"/>
                                            </t>
                                            <t t-set="t_transit" t-value="t_transit + item.transit"/>
                                            <t t-set="t_nac" t-value="t_nac + item.nac"/>
                                            <t t-set="t_total" t-value="t_total + item.total"/>
                                        </t>
                                    </t>
                                        <tr>
                                            <td colspan="6" class="text-right" style="border-bottom: 1px solid #cacaca;"><b style="font-size:12px;">Totals...</b></td>
                                            <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-right"><b style="font-size:12px;"><t t-esc="t_cont" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></b></td>
                                            <td colspan="4" class="text-right" style="border-bottom: 1px solid #cacaca;"></td>
                                            <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-right"><span style="font-size:12px;"><t t-esc="t_transit" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span></td>
                                            <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-right"><span style="font-size:12px;"><t t-esc="t_nac" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span></td>
                                            <td style="border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-right"><span style="font-size:12px;"><t t-esc="t_total" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span></td>
                                        </tr>
                                        <tr>
                                            <td colspan="11" class="text-right"></td>
                                            <td colspan="2" style="border-left: 1px solid #cacaca; border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;" class="text-right"><span style="font-size:12px;"><t t-esc="t_transit + t_nac" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span></td>
                                            <td class="text-right" style="border-right: 1px solid #cacaca; border-bottom: 1px solid #cacaca; display: table-cell; vertical-align: middle;"></td>
                                        </tr>
                                        <tr></tr>
                                        <tr class="border border-dark">
                                            <td colspan="2" class="text-center" style="background-color: #cacaca; display: table-cell; vertical-align: middle;"><span style="font-size:12px;">Average Production</span></td>
                                            <td class="text-right border-right border-dark" style="display: table-cell; vertical-align: middle;"><span style="font-size:12px;"><t t-esc="t_accumulated / t_cont" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span></td>
                                        </tr>
                                        <tr class="border border-dark">
                                            <td colspan="2" class="text-center" style="background-color: #cacaca; display: table-cell; vertical-align: middle;"><span style="font-size:12px;">Average Transit</span></td>
                                            <td class="text-right border-right border-dark" style="display: table-cell; vertical-align: middle;"><span style="font-size:12px;"><t t-esc="(t_transit + t_nac) / len(o.get_imports())" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span></td>
                                        </tr>
                                        <tr class="border border-dark">
                                            <td colspan="2" class="text-center" style="background-color: #cacaca; display: table-cell; vertical-align: middle;"><span style="font-size:12px;">Total Average Days</span></td>
                                            <td class="text-right border-right border-dark" style="display: table-cell; vertical-align: middle;"><span style="font-size:12px;"><t t-esc="(t_accumulated / t_cont) + ((t_transit + t_nac) / len(o.get_imports()))" t-options='{"widget": "float", "decimal_precision": "o.company_id.currency_id"}'/></span></td>
                                        </tr>
                                </tbody>
                            </table>

                        </div>
                    </t>
                </t>
            </t>
        </template>

</odoo>
